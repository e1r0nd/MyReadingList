{
  "name": "my-reading-list",
  "version": "0.0.1",
  "author": "Anatol Marezhanyi aka e1r0nd//[CRG]",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "codacy-coverage": "^2.0.3",
    "enzyme": "^2.8.2",
    "eslint": "^4.17.0",
    "eslint-plugin-babel": "^4.1.2",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "node-sass-chokidar": "0.0.3",
    "prettier": "^1.10.2",
    "react-addons-test-utils": "^15.5.1",
    "react-scripts": "^1.0.2",
    "snyk": "^1.69.6"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.5.10",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-md": "^1.2.11",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.5.2",
    "todomvc-app-css": "^2.1.0",
    "webfontloader": "^1.6.28"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
      ]
    },
  "scripts": {
    "precommit": "lint-staged",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "eject": "react-scripts eject",
    "test": "react-scripts test --env=jsdom",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "codacy-coverage": "cat ./coverage/lcov.info | node ./node_modules/.bin/codacy-coverage",
    "coveralls-win": "cat .\\coverage\\lcov.info | node .\\node_modules\\codacy-coverage\\bin\\codacy-coverage"
  },
  "snyk": true
}
